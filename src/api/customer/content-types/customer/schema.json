{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "unique": true
    },
    "phone": {
      "type": "biginteger",
      "unique": true
    },
    "password": {
      "type": "password"
    },
    "cars": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::car.car"
    },
    "rides": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ride.ride"
    }
  }
}
